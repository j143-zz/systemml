op1 = $1;
op2 = $2;
type = $3;

trueVar = TRUE;
b = FALSE;

if( type == 0 ) {
   b = xor(op1, op2); # xor(0, 0) = 1
}
else if( type == 1 ) {
   b = xor(op1, op2)  # xor(0, 1) = 0
}
else if( $3 == 2 ) {
   b = xor(op1, op2)  # xor(1, 0) = 0
}
else if( type == 3 ) {
   b = xor(op1, op2)  # xor(1, 0) = 0
}
else if( type == 4 ) {
   b = xor(op1, op2)  # xor(1, 1) = 1
}
else if( type == 5 ) {
   b = xor(op1, op2)  # xor(1, 0) = 0
}
else if( type == 6 ) {
   b = xor(op1, op2)  # xor(1, 1) = 1
}
else if( type == 7) {
   b = xor(op1, op2)  # xor(0, 1) = 0
}

write(b, $4);
